; Use PHP 5.6 or PHP 7 mode for HHVM
; PHP 5.6 Mode
hhvm.php7.all = 0
; PHP 7 Mode (default)
;hhvm.php7.all = 1

[PHP]
; Memory
memory_limit			= 4096M

; Timeouts
max_execution_time		= 180
max_input_time			= 180
; Uploads
post_max_size			= 1990M
upload_max_filesize		= 1990M

; Vars
max_input_vars			= 8000

; Error reporting
error_reporting			= E_ALL
display_errors			= On
track_errors			= On
